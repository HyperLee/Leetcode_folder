{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/leetcode_328/bin/Debug/net10.0/leetcode_328.dll",
      "args": ["Alice"],
      "cwd": "${workspaceFolder}/leetcode_328",
      "console": "integratedTerminal",
      "stopAtEntry": false,
      "internalConsoleOptions": "neverOpen",
      "env": {
        "NAME": "Alice"
      }
    }
  ]
}
