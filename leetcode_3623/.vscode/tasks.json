    },
    {
      "label": "run",
      "type": "shell",
      "command": "dotnet",
      "args": [
        "run",
        "--project",
        "${workspaceFolder}/leetcode_3623/leetcode_3623.csproj",
        "--no-build",
        "-c",
        "Debug"
      ],
      "problemMatcher": [],
      "group": {
        "kind": "none",
        "isDefault": false
      }
    }
  ]
}
