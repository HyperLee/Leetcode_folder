{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build - leetcode_1262",
      "type": "process",
      "command": "dotnet",
      "args": [
        "build",
        "${workspaceFolder}/leetcode_1262/leetcode_1262.csproj",
        "-c",
        "Debug"
      ],
      "problemMatcher": ["$msCompile"],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },

    {
      "label": "run - leetcode_1262",
      "type": "process",
      "command": "dotnet",
      "args": [
        "run",
        "--project",
        "${workspaceFolder}/leetcode_1262/leetcode_1262.csproj",
        "--no-build",
        "--configuration",
        "Debug"
      ],
      "presentation": {
        "reveal": "always",
        "panel": "shared"
      }
    }
  ]
}
